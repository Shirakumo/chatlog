<form class="nav" method="get">
  <a href="#" title="Go backward by a day." lquery="(attr :href (chatlog::time-link (- from 86400) types :from))" class="backward">
    <i class="fa fa-backward fa-fw"></i>
  </a>
  <span>
    <input type="datetime" name="from" title="Lower Time Limit (ie 2014-01-01T00:00:00)" placeholder="2014-01-01T00:00:00"
	   lquery="(val (chatlog::format-long-time from))" />
    <input type="datetime" name="to" title="Upper Time Limit (ie 2014-01-01T12:00:00)" placeholder="2014-01-01T12:00:00"
	   lquery="(val (chatlog::format-long-time to))" />
    <span class="checkbox" title="Messages">
      <input type="checkbox" name="type[]" id="type-m" value="m" lquery="(attr :checked (find #\m types))" />
      <label for="type-m" />
    </span>
    <span class="checkbox" title="Actions">
      <input type="checkbox" name="type[]" id="type-a" value="a" lquery="(attr :checked (find #\a types))" />
      <label for="type-a" />
    </span>
    <span class="checkbox" title="Nick Changes">
      <input type="checkbox" name="type[]" id="type-n" value="n" lquery="(attr :checked (find #\n types))" />
      <label for="type-n" />
    </span>
    <span class="checkbox" title="Joins">
      <input type="checkbox" name="type[]" id="type-j" value="j" lquery="(attr :checked (find #\j types))" />
      <label for="type-j" />
    </span>
    <span class="checkbox" title="Quits">
      <input type="checkbox" name="type[]" id="type-q" value="q" lquery="(attr :checked (find #\q types))" />
      <label for="type-q" />
    </span>
    <span class="checkbox" title="Partings">
      <input type="checkbox" name="type[]" id="type-p" value="p" lquery="(attr :checked (find #\p types))" />
      <label for="type-p" />
    </span>
    <span class="checkbox" title="Kicks">
      <input type="checkbox" name="type[]" id="type-k" value="k" lquery="(attr :checked (find #\l types))" />
      <label for="type-k" />
    </span>
    <span class="checkbox" title="Mode Changes">
      <input type="checkbox" name="type[]" id="type-o" value="o" lquery="(attr :checked (find #\o types))" />
      <label for="type-o" />
    </span>
    <span class="checkbox" title="Topic Changes">
      <input type="checkbox" name="type[]" id="type-t" value="t" lquery="(attr :checked (find #\t types))" />
      <label for="type-t" />
    </span>
    <input type="submit" value="Filter" />
    <span class="spring"/>
  </span>
  <a href="#" title="Go forward by a day." lquery="(attr :href (chatlog::time-link (+ from 86400) types :from))" class="forward">
    <i class="fa fa-forward fa-fw"></i>
  </a>
</form>
